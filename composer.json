{
    "name": "minasm/laravel-revisions",
    "description": "An update for from neurony. Create multiple revisions of any Eloquent model record along with its underlying relationships",
    "license": "MIT",
    "keywords": [
        "laravel",
        "revisions",
        "eloquent",
        "model",
        "relations",
        "changes",
        "copy"
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Minas Miroglu",
            "email": "braid_heading.0v@icloud.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/contracts": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5 || ^11.0.1",
        "orchestra/testbench": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "Neurony\\Revisions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Neurony\\Revisions\\Tests\\": "tests/"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Neurony\\Revisions\\ServiceProvider"
            ]
        }
    }
}
